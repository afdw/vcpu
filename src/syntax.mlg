DECLARE PLUGIN "vcpu-plugin.plugin"

{
  open Stdarg
}

VERNAC COMMAND EXTEND MyDefine CLASSIFIED AS SIDEFF
| [ "Compile" ident(input_id) ] ->
  {
    Compiler.compile input_id [] None
  }
| [ "Compile" ident(input_id) "of" constr_list(param_constr_exprs) "as" ident(output_id) ] ->
  {
    Compiler.compile input_id param_constr_exprs (Some output_id)
  }
END
